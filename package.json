{
  "name": "@sailfin/tekton",
  "version": "1.0.12",
  "description": "A Tekton and K8s client for Node.js",
  "author": "Michael Curtis <mpbcurtis@gmail.com>",
  "readme": "README.md",
  "contributors": [
    {
      "name": "Michael Curtis",
      "email": "mpbcurtis@gmail.com",
      "url": "https://github.com/mcereal"
    }
  ],
  "maintainers": [
    {
      "name": "Michael Curtis",
      "email": "mpbcurtis@gmail.com",
      "url": "https://github.com/mcereal"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SailfinIO/tekton/issues"
  },
  "homepage": "https://github.com/SailfinIO/tekton#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SailfinIO/tekton.git"
  },
  "keywords": [
    "tekton",
    "kubernetes",
    "typescript",
    "nodejs"
  ],
  "scripts": {
    "build": "npm run clean && tsc -b tsconfig.build.json",
    "test": "jest --detectOpenHandles --forceExit --config jest.config.ts",
    "e2e-test": "jest --detectOpenHandles --forceExit --config jest.e2e.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:coverage": "jest --detectOpenHandles --forceExit --config jest.config.ts --coverage",
    "start:dev": "cd testconnection && nest start --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "doc": "typedoc"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mcereal"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}
